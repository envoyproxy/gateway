backends:
- kind: Backend
  metadata:
    creationTimestamp: null
    name: {{.BackendName}}
    namespace: envoy-gateway-system
  spec:
    endpoints:
    - ip:
        address: 0.0.0.0
        port: 3000
  status: {}
gatewayClass:
  kind: GatewayClass
  metadata:
    creationTimestamp: null
    name: {{.GatewayClassName}}
    namespace: envoy-gateway-system
  spec:
    controllerName: gateway.envoyproxy.io/gatewayclass-controller
  status: {}
gateways:
- kind: Gateway
  metadata:
    creationTimestamp: null
    name: {{.GatewayName}}
    namespace: envoy-gateway-system
  spec:
    gatewayClassName: {{.GatewayClassName}}
    listeners:
    - name: http
      port: 8888
      protocol: HTTP
  status: {}
httpRoutes:
- kind: HTTPRoute
  metadata:
    creationTimestamp: null
    name: {{.HTTPRouteName}}
    namespace: envoy-gateway-system
  spec:
    hostnames:
    - www.example.com
    parentRefs:
    - name: {{.GatewayName}}
    rules:
    - backendRefs:
      - group: gateway.envoyproxy.io
        kind: Backend
        name: {{.BackendName}}
      matches:
      - path:
          type: PathPrefix
          value: /
  status:
    parents: null
namespaces:
- metadata:
    creationTimestamp: null
    name: envoy-gateway-system
  spec: {}
  status: {}
